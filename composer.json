{
    "name": "ahebrank/zendservice-slideshare",
    "description": "OOP wrapper for the SlideShare web service",
    "type": "library",
    "keywords": [
        "zf2",
        "slideshare",
        "slide",
        "share"
    ],
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-0": {
            "ZendService\\SlideShare\\": "library/"
        }
    },
    "require": {
        "php": ">=7.4",
        "laminas/laminas-xml": "^1",
        "laminas/laminas-http": ">=2.0.0",
        "laminas/laminas-uri": ">=2.0.0",
        "laminas/laminas-cache": ">=2.0.0",
        "laminas/laminas-serializer": "^2.7",
        "laminas/laminas-dependency-plugin": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
